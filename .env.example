# Base URL for the A2E API (example: https://api.example.com)
A2E_API_BASE=
# Secret API key for A2E
A2E_API_KEY=
# Google API key for Gemini / Imagen 3
GOOGLE_API_KEY=
# Optional server port (default 8080)
PORT=8080
# Optional: comma-separated list of allowed browser origins
ALLOWED_ORIGINS=http://localhost:8080
# Optional: max request body size in bytes (default 20000000)
MAX_BODY_BYTES=20000000
